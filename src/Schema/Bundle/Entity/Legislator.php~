<?php

namespace Schema\Bundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Legislator
 *
 * @ORM\Table(name="legislator", indexes={@ORM\Index(name="IDX_A7DD3A46B08FA272", columns={"district_id"})})
